#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int  d,m,y,n;
    scanf("%d\n%d\n%d\n%d",&d,&m,&y,&n);
     if((d+m+y)%3==0 && (d+m+y)%4==0)
     {
         if(n>50 && n<100)
         {
             printf("Cinema Day");
         }
     }
    return 0;
}
