#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     
    char s[200];
    int i;
    scanf("%[^\n]s",s);
    for(i=0;s[i]!='\0';i++)
    {
      if((s[i]>=65 && s[i]<=90) ||(s[i]>=97 && s[i]<=122)|| s[i]==32)
      {
          printf("%c",s[i]);
         }
    }

    return 0;
}
