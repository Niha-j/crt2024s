#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[50];
    int i,max=0,min=122;
    scanf("%s",s);
    for(i=0;s[i]!='\0';i++)
    {
        if(max<s[i])
        {
           max=s[i]; 
        }
        if(min>s[i])
        {
            min=s[i];
        }
    }
    
    printf("%d",(min+max));
    return 0;
}
