#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char s[100];
    int i,n=0;
    scanf("%[^\n]s",s);
    for(i=0;s[i]!='\0';i++)
    {
      if((s[i]>=65 && s[i]<=90) || (s[i]>=97 && s[i]<=122))
      {
          
      }
        else if((s[i]>=48 && s[i]<=57 )||(s[i]==32))
        {
            
        }
        else
        {
            n++;
        }
    }
    printf("%d",n);
    return 0;
}
