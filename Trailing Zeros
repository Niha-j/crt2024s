#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,p=1,c=0;
    scanf("%d",&n);
    while(n>=2)
    {
        p=p*n;
        n--;
    }
    int x=p%10;
    while(x==0)
    {
        c++;
        p=p/10;
        x=p%10;
    }
    printf("%d",c);
    
    
    return 0;
}
