#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

     int r1=1,r2=1,n,i,r3;
    scanf("%d",&n);
    if(n==1||n==2){
        printf("1");
    } 
    else{
    for(i=1;i<=n-2;i++)
    {
       r3=r1+r2;
        r1=r2;
        r2=r3;
    }
    printf("%d",r3);
    }
    
    return 0;
}
